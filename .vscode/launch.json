{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch OpenQ4 (Debug)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\build_debug\\Debug\\OpenQ4.exe",
      "args": [
        "+set",
        "logFile",
        "2",
        "+set",
        "logFileName",
        "logs/openq4.log",
        "+set",
        "developer",
        "1",
        "+set",
        "r_fullscreen",
        "0",
        "+set",
        "g_autoScreenshot",
        "0",
        "+set",
        "g_autoScreenshotDelayMs",
        "10000",
        "+set",
        "g_autoScreenshotQuit",
        "0",
        "+set",
        "fs_basepath",
        "C:\\Program Files (x86)\\Steam\\steamapps\\common\\Quake 4",
        "+set",
        "fs_savepath",
        "C:\\Program Files (x86)\\Steam\\steamapps\\common\\Quake 4",
        "+set",
        "fs_devpath",
        "",
        "+set",
        "net_serverDedicated",
        "0",
        "+set",
        "net_serverAllowServerMod",
        "1",
        "+set",
        "si_pure",
        "0",
        "+set",
        "net_serverAllowServerMod",
        "1",
        "+set",
        "si_gameType",
        "DM",
        "+spawnServer",
        "mp/q4dm1"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
